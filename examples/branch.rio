main* = for () be
    describe 2
    describe -1
    describe 0
end

describe = for i:Int32 be
    print (description i)
end

description = for i:Int32 be
    message = branch with
        i < 0 to "negative"
        i > 0 to "positive"
        else "zero"
    end
    message
end

less = for i:Int32 j:Int32 be i < j
