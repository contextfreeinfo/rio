main* = for void be
    describe 2
    describe -1
    describe 0
end

describe = for i:Int be
    log of description i
end

description = for i:Int be
    message = branch of
        i < 0 to "negative"
        i > 0 to "positive"
        true to "zero"
    end
    message
end

less = for i:Int j:Int be i < j
