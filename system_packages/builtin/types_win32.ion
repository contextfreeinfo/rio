@foreign("wchar_t")
typedef wchar = uint16;

#static_assert(sizeof(long) == 4)
#static_assert(sizeof(ulong) == 4)

@foreign
const WCHAR_MIN = UINT16_MIN;

@foreign
const WCHAR_MAX = UINT16_MAX;

@foreign
const LONG_MIN = long(INT32_MIN);

@foreign
const LONG_MAX = long(INT32_MAX);

const ULONG_MIN = ulong(INT32_MIN);

@foreign
const ULONG_MAX = ulong(INT32_MAX);
