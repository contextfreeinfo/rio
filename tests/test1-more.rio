greet proc(name: string, age: int)
  printf("%s, %s!\n", message, name)
  printf("You are %td years old.\n", age)
end

report_scores proc(scores: [float])
  for scores do(score)
    printf("score: %f\n", score)
  end
end

show_person proc(person: Person)
  printf("%s is %td years old (", person.name, person.age)
  if
    person.age >= 65
      printf("senior")
    end
    person.age < 18
      printf("minor")
    end
    else
      printf("adult")
    end
  end
  printf(")\n")
  report_scores(person.scores)
end

show_persons proc(persons: [Person])
  for persons do(person)
    show_person(person)
  end
end
